{
	"authors": [
		"Ralph Alexander Bariz"
	],
	"lflags-dmd": [
		"-l:libphobos2.so"
	],
	"lflags-ldc": [
		"-l:libphobos2-ldc.so"
	],
	"importPaths": [
		"source/"
	],
	"dependencies": {
		"libevent": "~>2.0.2+2.0.16",
		"openssl": "~>1.1.5+1.0.1g"
	},
	"buildTypes": {
		"debug": {
			"buildOptions": ["debugMode", "debugInfo"],
			"dflags": [
				"-Hd=iface",
				"-op"
			]
		},
		"debug-profile": {
			"buildOptions": ["profile", "optimize", "inline", "debugInfo"],
			"dflags": [
				"-Hd=iface",
				"-op"
			]
		},
		"release": {
			"buildOptions": ["releaseMode", "optimize", "inline"],
			"dflags": [
				"-Hd=iface",
				"-op"
			]
		}
	},
	"name": "flow-core",
	"version": "0.2.0",
	"license": "GPL-3.0",
	"targetName": "flow-core",
	"targetType": "dynamicLibrary",
	"targetPath": "lib",
	"description": "FLOW runtime",
	"homepage": "https://github.com/RalphBariz/flow",
	"sourcePaths": [
		"source/"
	]
}