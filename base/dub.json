{
	"authors": [
		"Ralph Alexander Bariz"
	],
	"lflags": [
		"-l:libphobos2.so"
	],
	"importPaths": [
		"source/"
	],
	"buildTypes": {
		"debug": {
			"buildOptions": ["debugMode", "debugInfo"],
			"dflags": [
				"-Hd=iface",
				"-op"
			]
		},
		"debug-profile": {
			"buildOptions": ["profile", "optimize", "inline", "debugInfo"],
			"dflags": [
				"-Hd=iface",
				"-op"
			]
		},
		"release": {
			"buildOptions": ["releaseMode", "optimize", "inline"],
			"dflags": [
				"-Hd=iface",
				"-op"
			]
		}
	},
	"name": "flow-base",
	"version": "0.2.0",
	"license": "GPL-3.0",
	"targetName": "flow-base",
	"targetType": "dynamicLibrary",
	"targetPath": "lib",
	"description": "FLOW runtime",
	"homepage": "https://github.com/RalphBariz/flow",
	"sourcePaths": [
		"source/"
	]
}